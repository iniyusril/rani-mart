{
    "version": "0.2.0",
    "compounds": [{
        "name": "Launch & Debug",
        "configurations": ["Launch Program", "Launch localhost"]
    }],
    "configurations": [{
            "type": "php",
            "request": "launch",
            "name": "Launch Program",
            "cwd": "${workspaceRoot}",
            "port": 9000
        },
        {
            "name": "Launch localhost",
            "type": "pwa-chrome",
            "request": "launch",
            "url": "http://yusril.dev",
            "webRoot": "${workspaceRoot}"
        }
    ]
}
